{
  "compilerOptions": {
    "outDir": "./build/",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "lib":[
      "es2016",
      "dom"
    ]
  },
  "exclude": [
    "node_modules",
    "build",
    "dist"
  ]
}
